{
	"name": "otomad-helper",
	"displayName": "Otomad Helper",
	"version": "7.9.0",
	"description": "The front-end part of Otomad Helper extension.",
	"type": "module",
	"main": "./dist/index.html",
	"module": "esnext",
	"repository": {
		"type": "git",
		"url": "https://github.com/otomad/OtomadHelper.git"
	},
	"homepage": "https://otomadhelper.readthedocs.io/",
	"github": "https://github.com/otomad/OtomadHelper",
	"bugs": "https://github.com/otomad/OtomadHelper/issues",
	"author": "Rantetsu Inori",
	"project": "OTOMAD+",
	"license": "GPL-3.0-or-later",
	"private": true,
	"scripts": {
		"dev": "vite --host",
		"dev-force": "vite --host --force",
		"dev-delete-auto-types": "pnpm run delete-auto-types && vite --host --force",
		"build": "vite build",
		"preview": "vite preview",
		"prebuild": "scripts\\prebuild.bat",
		"delete-auto-types": "rimraf -g src/types/auto-*.d.ts",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"lint:css": "stylelint './src/**/*.{js,jsx,ts,tsx,css,scss,sass,less,styl}'",
		"release": "pnpm run build && gh release create $(node -pe \"require('./package.json').version\") $(node -pe \"require('./package.json').main\")",
		"compact-svg": "tsx scripts/compact-svg.ts",
		"crowdin:update": "tsx scripts/crowdin/update.ts",
		"crowdin:download": "tsx scripts/crowdin/download.ts",
		"list-credit-licenses": "tsx scripts/list-credit-licenses.ts",
		"prepare": "husky",
		"commit": "git-cz -a",
		"supported-browsers": "(echo export default && browserslist-useragent-regexp --allowHigherVersions) > src/supportedBrowsers.ts"
	},
	"browserslist": [
		"Edge >= 133",
		"Chrome >= 133"
	],
