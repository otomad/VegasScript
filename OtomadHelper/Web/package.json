{
	"name": "otomad-helper",
	"displayName": "Otomad Helper",
	"version": "7.9.0",
	"description": "The front-end part of Otomad Helper extension.",
	"type": "module",
	"main": "./dist/index.html",
	"module": "esnext",
	"repository": {
		"type": "git",
		"url": "https://github.com/otomad/OtomadHelper.git"
	},
	"homepage": "https://otomadhelper.readthedocs.io/",
	"github": "https://github.com/otomad/OtomadHelper",
	"bugs": "https://github.com/otomad/OtomadHelper/issues",
	"author": "Rantetsu Inori",
	"project": "OTOMAD+",
	"license": "GPL-3.0-only",
	"scripts": {
		"dev": "vite --host",
		"dev-force": "vite --host --force",
		"dev-delete-auto-types": "pnpm run delete-auto-types && vite --host --force",
		"build": "vite build",
		"preview": "vite preview",
		"prebuild": "scripts\\prebuild.bat",
		"delete-auto-types": "rimraf -g src/types/auto-*.d.ts",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"lint:css": "stylelint './src/**/*.{js,jsx,ts,tsx}'",
		"release": "pnpm run build && gh release create $(node -pe \"require('./package.json').version\") $(node -pe \"require('./package.json').main\")",
		"compact-svg": "tsx scripts/compact-svg.ts",
		"crowdin:update": "tsx scripts/crowdin/update.ts",
		"crowdin:download": "tsx scripts/crowdin/download.ts",
		"list-credit-licenses": "tsx scripts/list-credit-licenses.ts",
		"prepare": "husky",
		"commit": "git-cz -a",
		"supportedBrowsers": "(echo export default && browserslist-useragent-regexp --allowHigherVersions) > src/supportedBrowsers.ts"
	},
	"browserslist": [
		"Edge >= 129",
		"Chrome >= 129"
	],
	"dependencies": {
		"@dnd-kit/core": "^6.1.0",
		"@dnd-kit/modifiers": "^7.0.0",
		"@dnd-kit/sortable": "^8.0.0",
		"ani-cursor": "^0.0.5",
		"classnames": "^2.5.1",
		"dedent": "^1.5.3",
		"i18next": "^23.16.4",
		"i18next-browser-languagedetector": "^8.0.0",
		"immer": "^10.1.1",
		"jotai": "^2.10.1",
		"jotai-immer": "^0.4.1",
		"jotai-valtio": "^0.5.0",
		"lodash-es": "^4.17.21",
		"lottie-web": "^5.12.2",
		"mitt": "^3.0.1",
		"pangu": "^4.0.7",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-flip-move": "^3.0.5",
		"react-i18next": "^15.1.0",
		"react-transition-group": "file:/react-transition-group/",
		"styled-components": "^6.1.8",
		"styled-tools": "^1.7.2",
		"use-immer": "^0.10.0",
		"valtio": "^2.1.2"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.4.1",
		"@commitlint/config-conventional": "^19.4.1",
		"@commitlint/cz-commitlint": "^19.5.0",
		"@commitlint/types": "^19.0.3",
		"@crowdin/crowdin-api-client": "^1.33.1",
		"@styled/typescript-styled-plugin": "^1.0.1",
		"@stylistic/eslint-plugin": "^2.9.0",
		"@stylistic/stylelint-plugin": "^3.1.1",
		"@tonejs/midi": "^2.0.28",
		"@types/jsdom": "^21.1.7",
		"@types/license-checker": "^25.0.6",
		"@types/lodash-es": "^4.17.12",
		"@types/node": "latest",
		"@types/pangu": "^4.0.2",
		"@types/react": "^18.2.78",
		"@types/react-dom": "^18.2.25",
		"@types/react-transition-group": "^4.4.10",
		"@vitejs/plugin-react": "^4.2.1",
		"babel-plugin-styled-components": "^2.1.4",
		"babel-plugin-transform-jsx-classnames": "^1.2.2",
		"click-to-react-component": "^1.1.2",
		"commitizen": "^4.3.0",
		"eslint": "^9.13.0",
		"eslint-plugin-react": "^7.37.2",
		"eslint-plugin-react-hooks": "^5.0.0",
		"eslint-plugin-react-refresh": "^0.4.14",
		"extract-zip": "^2.0.1",
		"husky": "^9.1.5",
		"jsdom": "^24.1.1",
		"license-checker": "^25.0.1",
		"lightningcss": "^1.24.1",
		"postcss": "^8.4.47",
		"postcss-styled-syntax": "^0.6.4",
		"rimraf": "^6.0.1",
		"rollup-plugin-license": "^3.5.3",
		"sharp": "^0.33.5",
		"stylelint": "^16.10.0",
		"stylelint-config-standard": "^36.0.0",
		"stylelint-order": "^6.0.4",
		"svgo": "^3.3.2",
		"terser": "^5.30.3",
		"typescript": "latest",
		"typescript-eslint": "^8.11.0",
		"unplugin-auto-import": "^0.18.3",
		"unplugin-turbo-console": "^1.10.1",
		"vite": "^5.2.8",
		"vite-plugin-glsl": "^1.3.0",
		"vite-plugin-html-minifier": "^1.0.4",
		"vite-plugin-image-optimizer": "^1.1.8",
		"vite-plugin-svg-icons": "^2.0.1",
		"vite-plugin-svgr": "^4.2.0",
		"vite-tsconfig-paths": "^4.3.2",
		"xmlbuilder2": "^3.1.1"
	},
	"pnpm": {
		"patchedDependencies": {
			"babel-plugin-styled-components@2.1.4": "patches/babel-plugin-styled-components@2.1.4.patch",
			"babel-plugin-transform-jsx-classnames@1.2.2": "patches/babel-plugin-transform-jsx-classnames@1.2.2.patch",
			"lottie-web@5.12.2": "patches/lottie-web@5.12.2.patch",
			"pangu@4.0.7": "patches/pangu@4.0.7.patch",
			"postcss-styled-syntax@0.6.4": "patches/postcss-styled-syntax@0.6.4.patch",
			"react@18.2.0": "patches/react@18.2.0.patch",
			"react-dom@18.2.0": "patches/react-dom@18.2.0.patch",
			"react-flip-move@3.0.5": "patches/react-flip-move@3.0.5.patch",
			"@types/react@18.2.78": "patches/@types__react@18.2.78.patch",
			"styled-components@6.1.8": "patches/styled-components@6.1.8.patch",
			"postcss-selector-parser@6.1.2": "patches/postcss-selector-parser@6.1.2.patch",
			"postcss@8.4.47": "patches/postcss@8.4.47.patch",
			"vite-plugin-image-optimizer@1.1.8": "patches/vite-plugin-image-optimizer@1.1.8.patch"
		},
		"allowNonAppliedPatches": false
	},
	"config": {
		"commitizen": {
			"path": "@commitlint/cz-commitlint"
		}
	}
}
